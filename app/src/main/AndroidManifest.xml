        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_aplikasi"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_aplikasi"
        android:supportsRtl="true"
        android:theme="@style/Theme.GrowHarvest"
        tools:targetApi="31">
        <activity
            android:name=".Transaksi.RiwayatTransaksi"
            android:exported="false" />
        <activity
            android:name=".LoginActivity.DataAkun"
            android:exported="false" />
        <activity
            android:name=".PembayaranActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".LandingPageView.LandingPageMainActivity"
            android:exported="false" />
        <activity
            android:name=".Sementara.AccountActivity"
            android:exported="false" />
        <activity
            android:name=".Sementara.DataProductActivity"
            android:exported="false" />
        <activity
            android:name=".Sementara.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity.LoginBiasaActivity"
            android:exported="false" />
        <activity
            android:name=".Transaksi.DaftarProdukActivity"
            android:exported="false" />
        <activity
            android:name=".Transaksi.Pembayaran"
            android:exported="false" />
        <activity
            android:name=".LoginActivity.LoginMainActivity"
            android:exported="false" />
        <activity
            android:name=".Transaksi.KeranjangActivity"
            android:exported="false" />
        <activity
            android:name=".DetailProduk.DetailProdukActivity"
            android:label="Detail Produk"
            android:parentActivityName=".Transaksi.DaftarProdukActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Transaksi.DaftarProdukActivity" />
        </activity>
        <activity
            android:name=".SplashView.SplashMainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>